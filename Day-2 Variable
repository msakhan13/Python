name = input("What is your name? :")
my_age = input("How old are you? :")
print(type(my_age))
current_year = input("Enter current year= ")
print(type(current_year))
future_year = input("Enter a future year= ")
print(type(future_year))
future_age = ((int(future_year) - int(current_year) + int(my_age)))
print("Hello " + str(name) + "! " + "Your age at the year " + str(future_year) + " is " + str(future_age))
